    },
    "teeth": {
      "collection": "tooth",
      "via": "client"
    },
    "phone": {
      "type": "biginteger"
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    }
  }
}
